import java.io.*;
import org.codehaus.plexus.util.FileUtils;

try
{
    File file = new File( basedir, "build.log" );
    String buf = FileUtils.fileRead( file, "UTF-8" );

	String[] expectedErrors = new String[] {
		"[LINT] VERSIONPROPHYPHEN: Version property names must not contain a hyphen: 'dummy-plugin-version'",
		"[LINT] VERSIONPROPHYPHEN: Version property names must not contain a hyphen: 'dummy-impl-version'",
		"[LINT] VERSIONPROPHYPHEN: Version property names must not contain a hyphen: 'dummy-api-version'" };

	boolean passed = true;		
	for (String expectedError : expectedErrors) {
	    if ( buf.indexOf( expectedError ) < 0 )
	    {
	        System.err.println( "Did not find expected violation: " + expectedError );
	        passed = false;
	    }
    }
    return passed;
    
}
catch( Throwable t )
{
    t.printStackTrace();
    return false;
}

return true;
